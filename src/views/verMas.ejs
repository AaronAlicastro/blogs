    <div id="containerComentarios">
        <h2>Comentarios</h2>
        <% for(let i = 0; i < blogs.commentBlog.comments.length; i++){ %>
            <div id="comentarios_comentarios">
                <div class="coment">
                    <h3><%=blogs.commentBlog.comments[i].userName%></h3>
                    <textarea readonly> <%=blogs.commentBlog.comments[i].comentario%> </textarea>
                </div>
            </div>
        <% } %>

        <h3>A&ntildeade un comentario</h3>
        <form id="formComentario">
            <div class="centarOpciones">
                <input class="formComentario_opciones" type="text" name="nameUsuario" placeholder="Ingresa tu nombre">
            </div>
            <textarea class="formComentario_opciones" name="comentario" placeholder="Type your comment"></textarea>
            <div class="centarOpciones">
                <button class="formComentario_opciones" type="submit" id="enviarComentario">Enviar</button>
            </div>
        </form>
    </div>

    <script>
        $("#enviarComentario").click(e=>{
            e.preventDefault();
            $.ajax({
                url: "/anadirComentario/<%=blogs._id%>",
                type: "post",
                data: $("#formComentario").serialize(),
                success: response=>{
                    history.go(0);
                }
            });
            $("#formComentario").trigger("reset");
        });
    </script>
